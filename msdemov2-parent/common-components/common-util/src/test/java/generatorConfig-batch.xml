<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
  <context id="batch" defaultModelType="flat" targetRuntime="MyBatis3Simple"><!--MyBatis3,MyBatis3Simple -->
    <commentGenerator>
	    <property name="suppressAllComments" value="true"/>
	    <property name="suppressDate" value="true"/>
	</commentGenerator>
	
    <jdbcConnection connectionURL="jdbc:mysql://centos:3306/batch?allowMultiQueries=true" 
    	driverClass="com.mysql.cj.jdbc.Driver" 
    	password="batch" userId="batch" />
    
    <javaModelGenerator targetPackage="com.msdemo.v2.common.param.table.entity" targetProject="common-param" />
    <sqlMapGenerator targetPackage="com.msdemo.v2.common.param.table.mapper" targetProject="common-param" />
    <javaClientGenerator targetPackage="com.msdemo.v2.common.param.table.mapper" targetProject="common-param" type="XMLMAPPER" />
    
    
    <!-- <table schema="" tableName="TRANS_LOG" domainObjectName="TransLog"
    	enableCountByExample="false" 
    	enableUpdateByExample="true" 
    	enableDeleteByExample="false" 
    	enableSelectByExample="false" 
    	selectByExampleQueryId="false">
    </table> -->
    
    <table schema="" tableName="SYSTEM_PROPERTY" 
    	mapperName="SystemPropertyMapper"
    	domainObjectName="SystemProperty"
    	enableCountByExample="false" 
    	enableUpdateByExample="true" 
    	enableDeleteByExample="false" 
    	enableSelectByExample="true" 
    	selectByExampleQueryId="false">
    </table>
    <!-- <columnOverride column="???" property="???" /> -->
    
  </context>
</generatorConfiguration>